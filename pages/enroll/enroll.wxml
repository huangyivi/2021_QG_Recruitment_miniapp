<view class="enroll-container">
  <text class="enroll-title">报名QG工作室</text>
  <view class="infos">
    <text class="">报名</text>
    <input class="inputs" type="text" />
  </view>
  <view class="infos">
    <text class="">学号</text>
    <input class="inputs" type="number" />
  </view>
  <view class="infos">
    <text class="">绩点</text>
    <input class="inputs" type="number" />
  </view>
  <view class="infos">
    <text class="">学院</text>
    <picker class="inputs" range="{{colleges}}">{{myCollege}}</picker>
  </view>
  <view class="infos">
    <text class="">专业班级</text>
    <picker class="inputs" range="{{majors}}">{{myMajor}}</picker>
  </view>
  <view class="infos">
    <text class="">电话号码</text>
    <input  class="inputs" type="number" />
  </view>
  <view class="infos">
    <text class="">QQ</text>
    <input class="inputs" type="number" />
  </view>
  <view class="infos reason">
    <text class="">是否参加其他学生科技团队，社团参与情况说明，是否任职团委</text>
    <view>
      <textarea maxlength="150" />
    </view>
  </view>
  <view class="infos">
    <text class="">想加入工作室的方向</text>
    <picker class="inputs" range="{{directions}}">{{myDirection}}</picker>
  </view>
  <view class="infos reason">
    <text class="">个人简介</text>
    <view>
      <textarea maxlength="150" />
    </view>
  </view>
  <button class="{{isFilled?'filled' : 'unfilled'}}">{{isFilled?'我想加入QG工作室！':'请填写完相关信息'}}</button>

</view>